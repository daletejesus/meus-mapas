generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Mapa {
  id        Int      @id @default(autoincrement())
  name      String
  dataCadastro DateTime @default(now())
  status    Boolean     @default(true)

  ponto Ponto[]
}

model Ponto {
  id        Int      @id @default(autoincrement())
  idPonto   Int
  name      String
  latitude  String
  longitude String
  dataCadastro DateTime @default(now())
  status    Boolean

  mapa   Mapa @relation(fields: [idPonto], references: [id])
}